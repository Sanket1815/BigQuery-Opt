# BigQuery Query Optimizer Configuration

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-bigquery-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Gemini AI Configuration  
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-pro

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO

# BigQuery Settings
DEFAULT_LOCATION=US
DRY_RUN=true

# MCP Server Settings
MCP_HOST=localhost
MCP_PORT=8000

# Documentation Crawler Settings
DOCS_BASE_URL=https://cloud.google.com/bigquery/docs
CRAWL_DELAY=1.0

# Vector Database Settings
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Performance Testing Settings
PERFORMANCE_TEST_ITERATIONS=3
PERFORMANCE_THRESHOLD=0.3

# Paths (relative to project root)
DOCS_OUTPUT_DIR=data/documentation
VECTOR_DB_PATH=data/vector_db